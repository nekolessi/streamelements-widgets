{
  "name": "Chat Bubbles",
  "id": "chat-bubbles",
  "version": "0.1.0",
  "description": "Cute neko-styled chat bubbles widget for StreamElements overlays.",
  "author": "nekolessi",
  "fields": [
    {
      "key": "theme",
      "type": "select",
      "options": [
        "light",
        "dark"
      ],
      "default": "dark"
    },
    {
      "key": "fontSize",
      "type": "number",
      "min": 10,
      "max": 64,
      "default": 24
    },
    {
      "key": "ignoredUsers",
      "type": "string",
      "default": ""
    }
  ],
  "preview": "demo/index.html"
}